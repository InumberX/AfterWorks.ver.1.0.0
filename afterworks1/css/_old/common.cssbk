  font-family: 'Noto Sans Japanese', serif;
  font-weight: 100;
}

.skill-table th{
  font-family: 'Noto Sans Japanese', serif;
  font-weight: 100;
  vertical-align: middle;
}

/* Certificate */
.certificate-table{
  margin: 10px 0px 0px 0px;
  width: 100%;
  font-family: 'Noto Sans Japanese', serif;
  font-weight: 100;
}

.certificate-table th{
  font-family: 'Noto Sans Japanese', serif;
  font-weight: 100;
}

/* Hobby */

}
